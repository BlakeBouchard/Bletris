{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "CL Build Debug",
            "type": "shell",
            "command": "powershell",
			"args": [
                "${workspaceFolder}/Build.ps1",
				"-Debug"
            ],
			"options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
				"$msCompile"
			],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}
